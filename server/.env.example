# LawPro Admin Server - Environment Variables
# Copy this file to .env and fill in the values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-service-role-key

# Admin Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-secure-password

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-change-in-production

# Server Configuration (Railway sets PORT automatically)
PORT=8000

# === Document Conversion API ===
# Upstage API Key (https://console.upstage.ai)
UPSTAGE_API_KEY=up_xxxxxxxxxxxxxxxxxxxx

# === Proxy Configuration (Decodo) ===
# Optional: For multi-user support with different IPs
PROXY_HOST=gate.decodo.com
PROXY_USERNAME=your-proxy-username
PROXY_PASSWORD=your-proxy-password
PROXY_PORT_START=10001
PROXY_PORT_END=10050
PROXY_MODE=round_robin
